{
	"name": "opengraphpics",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"push:staging": "sst deploy --stage staging",
		"push:production": "sst deploy --stage production",
		"remove:dev": "sst remove",
		"remove:staging": "sst remove --stage staging",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.651.1",
		"@aws-sdk/lib-dynamodb": "^3.651.1",
		"arctic": "^2.0.0-next.5",
		"bits-ui": "^0.21.15",
		"clsx": "^2.1.1",
		"jsonwebtoken": "^8.5.1",
		"lucide-svelte": "^0.441.0",
		"oslo": "^1.2.1",
		"sst": "^3.1.32",
		"svelte-kit-sst": "^2.43.5",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.0",
		"@fontsource/gochi-hand": "^5.0.20",
		"@fontsource/outfit": "^5.1.0",
		"@iconify-json/mdi": "^1.2.0",
		"@iconify-json/noto-v1": "^1.2.0",
